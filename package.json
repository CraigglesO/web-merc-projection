{
  "name": "web-merc-projection",
  "version": "1.0.0",
  "description": "Work with the web mercator projection.",
  "keywords": [
    "web",
    "mercator",
    "projection",
    "spherical",
    "s2maps",
    "S2Point",
    "S2CellID",
    "S2LonLat"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "typescript": {
    "main": "src/index.ts"
  },
  "directories": {
    "lib": "dist"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./mercCoords": {
      "types": "./dist/mercCoords.d.ts",
      "import": "./dist/mercCoords.js"
    },
    "./mercProjSpec": {
      "types": "./dist/mercProjSpec.d.ts",
      "import": "./dist/mercProjSpec.js"
    },
    "./util": {
      "types": "./dist/util.d.ts",
      "import": "./dist/util.js"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "dist/index.d.ts"
      ],
      "mercCoords": [
        "dist/mercCoords.d.ts"
      ],
      "mercProjSpec": [
        "dist/mercProjSpec.d.ts"
      ],
      "util": [
        "dist/util.d.ts"
      ]
    }
  },
  "scripts": {
    "lint+build+test": "pnpm run lint && pnpm run build && pnpm run test",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build": "rm -rf dist && mkdir dist && rm -f -- tsconfig.tsbuildinfo && tsc",
    "types:bundle": "npm run types:build && node scripts/types.mjs",
    "prepublish": "pnpm run lint+build+test",
    "package-check": "package-check",
    "test:dev": "vitest dev",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/S2-Maps/web-merc-projection"
  },
  "author": "Craig OConnor",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/S2-Maps/web-merc-projection/issues"
  },
  "homepage": "https://github.com/S2-Maps/web-merc-projection#readme",
  "devDependencies": {
    "@skypack/package-check": "^0.2.2",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "es-abstract": "^1.21.2",
    "eslint": "^8.44.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "protoscript": "^0.0.8",
    "ts-standard": "*",
    "typescript": "^5.1.6",
    "vitest": "^0.32.4"
  }
}
