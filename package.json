{
  "name": "s2projection",
  "version": "3.1.5",
  "description": "Work with spherical geometry and it's xyz vector normals.",
  "keywords": [
    "s2projection",
    "projection",
    "spherical",
    "s2maps",
    "S2Point",
    "S2CellID",
    "S2LonLat"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "typescript": {
    "main": "src/index.ts"
  },
  "directories": {
    "lib": "dist"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./rbTree": {
      "types": "./dist/rbTree.d.ts",
      "import": "./dist/rbTree.js"
    },
    "./s2CellID": {
      "types": "./dist/s2CellID.d.ts",
      "import": "./dist/s2CellID.js"
    },
    "./s2Coords": {
      "types": "./dist/s2Coords.d.ts",
      "import": "./dist/s2Coords.js"
    },
    "./s2LonLat": {
      "types": "./dist/s2LonLat.d.ts",
      "import": "./dist/s2LonLat.js"
    },
    "./s2Point": {
      "types": "./dist/s2Point.d.ts",
      "import": "./dist/s2Point.js"
    },
    "./util": {
      "types": "./dist/util.d.ts",
      "import": "./dist/util.js"
    }
  },
  "typesVersions": {
    "*": {
      "index": ["dist/index.d.ts"],
      "rbTree": ["dist/rbTree.d.ts"],
      "s2CellID": ["dist/s2CellID.d.ts"],
      "s2Coords": ["dist/s2Coords.d.ts"],
      "s2LonLat": ["dist/s2LonLat.d.ts"],
      "s2Point": ["dist/s2Point.d.ts"],
      "util": ["dist/util.d.ts"]
    }
  },
  "scripts": {
    "lint+build+test": "pnpm run lint && pnpm run build && pnpm run test",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build": "rm -rf dist && mkdir dist && rm -f -- tsconfig.tsbuildinfo && tsc",
    "types:bundle": "npm run types:build && node scripts/types.mjs",
    "prepublish": "pnpm run lint+build+test",
    "package-check": "package-check",
    "test": "tape test/**/*.test.js | tap-summary"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/S2-Maps/s2projection"
  },
  "author": "Craig OConnor",
  "license": "CC BY-NC-ND 3.0",
  "bugs": {
    "url": "https://github.com/S2-Maps/s2projection/issues"
  },
  "homepage": "https://github.com/S2-Maps/s2projection#readme",
  "devDependencies": {
    "@skypack/package-check": "^0.2.2",
    "@types/node": "^17.0.35",
    "@typescript-eslint/parser": "^4.0.0",
    "es-abstract": "^1.21.2",
    "eslint": "^8.44.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "tap-summary": "^4.0.0",
    "tape": "^5.6.4",
    "ts-standard": "*",
    "typescript": "^5.1.6"
  }
}
